@prefix lv2: <http://lv2plug.in/ns/lv2core#> .

<http://example.org/plugins/gain>
    a lv2:Plugin ;
    lv2:port [
        a lv2:AudioPort, lv2:InputPort ;
        lv2:index 0 ;
        lv2:symbol "input" ;
        lv2:name "Audio Input"
    ], [
        a lv2:AudioPort, lv2:OutputPort ;
        lv2:index 1 ;
        lv2:symbol "output" ;
        lv2:name "Audio Output"
    ], [
        a lv2:ControlPort, lv2:InputPort ;
        lv2:index 2 ;
        lv2:symbol "gain" ;
        lv2:name "Gain" ;
        lv2:default 1.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 2.0
    ] .

