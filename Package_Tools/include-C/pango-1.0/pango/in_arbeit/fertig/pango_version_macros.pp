  PANGO_VERSION_MIN_REQUIRED = PANGO_VERSION_CUR_STABLE;  
(*** was #elif ****){$else PANGO_VERSION_MIN_REQUIRED == 0}
{$undef  PANGO_VERSION_MIN_REQUIRED}

const
